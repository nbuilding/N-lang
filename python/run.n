let main = [_: ()] -> () {
    let results: list[result[int, str]] = [err("sad"), ok(2), err("wow")]

    let errors = filterMap(
        [result: result[int, str]] -> maybe[str] {
            return if let <err error> = result {
                yes(error)
            } else {
                none
            }
        },
        results
    )
        |> err
        |> yes

    print(errors)
}

main(())


if (true) {
	print(main)
}